{
	"SassCompiler": {
		"SourceFolder": "Styles",
		"TargetFolder": "wwwroot/css",
		"Arguments": "--style=compressed",

		"Configurations": {
			"Debug": {
				"Arguments": "--style=expanded"
			}
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*"
}
