{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"SassCompiler": {
		"SourceFolder": "wwwroot/scss",
		"TargetFolder": "wwwroot/css",
		"Arguments": "--style=compressed",
		"Configurations": {
			"Debug": {
				"Arguments": "--style=expanded"
			}
		}
	}
}
